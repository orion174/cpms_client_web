{
  "name": "cpms-wcs-client-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm run dev",
    "compile:scss": "sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css",
    "minify:scss": "sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.min.css --style compressed",
    "build:scss": "npm run compile:scss && npm run minify:scss && npm run map:scss"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "6.5.1",
    "ajv": "^8.16.0",
    "axios": "^1.7.5",
    "bootstrap": "4.6.2",
    "chart.js": "^4.1.1",
    "classnames": "2.3.2",
    "crypto-js": "^4.2.0",
    "moment": "2.29.4",
    "nouislider": "15.4.0",
    "react": "18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-copy-to-clipboard": "5.1.0",
    "react-datetime": "3.2.0",
    "react-dom": "18.2.0",
    "react-popper": "^2.3.0",
    "react-router-dom": "^6.21.1",
    "reactstrap": "8.10.0",
    "sass": "1.69.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^4.3.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "prettier": "^3.3.2",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "eslint",
      "parser": "typescript"
    }
  }
}
